generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Plante {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  quantity    Int
  addedDate   DateTime @default(now())
  isActive    Boolean  @default(true)
  imageUrl    String?
}
